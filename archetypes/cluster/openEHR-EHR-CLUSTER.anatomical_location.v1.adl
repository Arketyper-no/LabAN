				>
				["at0075"] = <
					text = <"*Palmar(en)">
					description = <"*Towards the palm of the hand.(en)">
				>
				["at0076"] = <
					text = <"*Plantar(en)">
					description = <"*Towards the sole of the foot.(en)">
				>
				["at0077"] = <
